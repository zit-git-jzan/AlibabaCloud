variable "access_key" {
  type        = string
  description = "Alibaba Cloud Access Key"
  default     = "123456"
}

variable "secret_key" {
  type        = string
  description = "Alibaba Cloud Secret Key"
  default     = "abcd"
}

variable "region" {
  type        = string
  description = "Alibaba Cloud Region"
  default     = "abcd"
}